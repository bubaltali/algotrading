        "    print(\"üöÄ QUANTITATIVE STRATEGY SYSTEM DEMO\")\n",
        "    print(\"=\" * 60)\n",
        "    SAMPLE_SIZE = 15\n",
        "    try:\n",
        "        # Initialize orchestrator\n",
        "        print(\"1Ô∏è‚É£ Initializing system...\")\n",
        "        orchestrator = QuantitativeStrategyOrchestrator()\n",
        "\n",
        "        # Generate system report\n",
