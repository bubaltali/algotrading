        "elif SAMPLE_SIZE <= 30:\n",
        "    category = \"Medium Dataset (16-30 stocks)\"\n",
        "else:\n",
        "    category = \"Large Dataset (31-50 stocks)\"\n",
        "\n",
        "config = configuration_guide[category]\n",
        "print(f\"üìä Current Category: {category}\")\n",
        "print(f\"üéØ Recommended Size: {config['recommended_size']}\")\n",
        "print(f\"üé™ Use Case: {config['use_case']}\")\n",
